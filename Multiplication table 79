#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{
 int n,i;
    scanf("%d,", &n);
    if(n >= 1 && n<=9)
    {
        for(i = 1; i<=n;i++)
        {
      printf("%d x %d = %d\n",n,i,n*i);
        }
                    }
    else
        {
        printf("Invalid Input\n");
        }
    return 0;
}
