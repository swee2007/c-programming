#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>


int main() {

    char name[50];

    int m1, m2, m3, m4, m5;

    int total;

    float average;

    

    scanf("%s", name);

    scanf("%d%d%d%d%d", &m1, &m2, &m3, &m4, &m5);

    

    total = m1 + m2 + m3 + m4 + m5;

    average = total / 5.0;

    printf("Name of the Student:%s\n", name);

    printf("Total Mark:%d\n", total);

    printf("Average Mark:%.1f\n", average);



    if (average == 100)

        printf("Grade Mark:S\n");

    else if (average >= 90)

        printf("Grade Mark:A\n");

    else if (average >= 80)

        printf("Grade Mark:B\n");

    else if (average >= 70)

        printf("Grade Mark:C\n");

    else if (average >= 60)

        printf("Grade Mark:D\n");

    else if (average >= 50)

        printf("Grade Mark:E\n");

    else

        printf("Grade Mark:Fail\n");

    return 0;

}
