#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int main() 
{
 int size;
    scanf("%d",&size);
    int even=0,odd=0;
    int n;
    for(int i=0;i<size;i++){
        scanf("%d",&n);
    if(n%2==0)
        even=1;
     else
         odd=1;
       }
    if(even&&odd)
        printf("Mixed\n");
    else if(even)
        printf("Even\n");
    else
        printf("Odd\n");
    return 0;
}