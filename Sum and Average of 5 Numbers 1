#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    int a,b,c,d,e;
    int sum;
    float average;
    scanf("%d %d %d %d %d",&a,&b,&c,&d,&e);
    sum=a+b+c+d+e;
    average=sum/5.0;
    printf("Sum of the 5 number is : %d\n",sum);
    printf("Average of the 5 number is : %.1f\n",average);
    return 0;
}
